@model Domain.appFacture.FACRAPPORTGEN

@{
    Layout = null;
    ViewBag.Title = "Rapport génération brouillard";
}



<div class="modal-header">
    <button title="Fermer cette fenêtre" type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
    <h4 class="modal-title">Rapport génération brouillard</h4>
</div>
<div class="modal-body">
    @if (Model.MESSAGES != null) { 
       foreach (String item in Model.MESSAGES)
        {
            <p>@item</p>  
        }
    }
</div>
